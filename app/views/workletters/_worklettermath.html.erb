        if (!unit){unit = 0;}
        var total = unit * sqft;
        if (!total){total = 0;}
            total = total.toFixed(2);
        var totalformatted = '$' + total.replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,");
