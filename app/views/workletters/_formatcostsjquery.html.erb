        var llworkcostformatted = '$' + llworkcostfixed.replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,");
        //change # to total
        $("#workletter_ll_work_cost").val(llworkcostformatted);
        //this input field is saved as a string
    }
    
